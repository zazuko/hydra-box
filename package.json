{
  "name": "hydra-box",
  "description": "Hydra Box middleware",
  "version": "0.6.6",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "lint": "eslint . --quiet --ignore-path .gitignore",
    "test": "c8 --all --reporter lcovonly --reporter text mocha",
    "build": "tsc",
    "preblog": "npm run build",
    "blog": "(cd examples/blog; bash init.sh --keep; DEBUG=hydra-box* nodemon --ext ttl,js,json --exec node --inspect=0.0.0.0:9001 server.js)",
    "prepack": "npm run build",
    "release": "changeset publish"
  },
  "author": "Thomas Bergwinkl <bergi@axolotlfarm.org> (https://www.bergnet.org/people/bergi/card#me)",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git://github.com/zazuko/hydra-box.git"
  },
  "bugs": {
    "url": "https://github.com/zazuko/hydra-box/issues"
  },
  "homepage": "https://github.com/zazuko/hydra-box",
  "dependencies": {
    "@rdfjs/express-handler": "^2.0.0",
    "@zazuko/env-node": "^2.1.1",
    "absolute-url": "^2.0.0",
    "debug": "^4.1.1",
    "express": "^4.17.1",
    "http-errors": "^2.0.0",
    "is-graph-pointer": "^2",
    "middleware-async": "^1.4.0",
    "promise-the-world": "^1.0.1",
    "rdf-loader-code": "^2.1.3",
    "rdf-loaders-registry": "^1.0.3",
    "set-link": "^1.0.0",
    "uri-template-route": "^1.0.0"
  },
  "devDependencies": {
    "@changesets/cli": "^2.26.1",
    "@tpluscode/eslint-config": "^0.4.5",
    "@types/absolute-url": "^2.0.0",
    "@types/debug": "^4.1.12",
    "@types/express": "^4.17.21",
    "@types/express-as-promise": "^1.2.0",
    "@types/mocha": "^10.0.6",
    "@types/promise-the-world": "^1.0.6",
    "@types/rdf-store-fs": "^1.1.4",
    "@types/rdfjs__express-handler": "^2.0.6",
    "@types/readable-stream": "^4.0.10",
    "@types/set-link": "^1.0.4",
    "@types/sinon": "^17.0.3",
    "@types/supertest": "^6.0.2",
    "@types/uri-template-route": "^1.0.0",
    "@typescript-eslint/eslint-plugin": "^7.0.1",
    "@typescript-eslint/parser": "^7.0.1",
    "alcaeus": "^3.0.0",
    "c8": "^9.1.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "express-as-promise": "0.0.2",
    "mocha": "^10.3.0",
    "node-fetch": "^3.3.2",
    "nodemon": "^3.1.0",
    "rdf-store-fs": "^1.1.0",
    "rimraf": "^5.0.5",
    "sinon": "^17",
    "supertest": "^6.3.4",
    "tsm": "^2.3.0",
    "typescript": "^5.3.3"
  },
  "mocha": {
    "extensions": ["ts"],
    "loader": "tsm"
  },
  "c8": {
    "all": true,
    "reporters": [
      "lcovonly",
      "text-summary"
    ],
    "exclude": [
      "coverage",
      "examples",
      "test"
    ]
  }
}
